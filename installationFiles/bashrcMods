##custom script dir start
export RESOURCEDIR="$HOME/resources"
export PATHPROGDIR="$RESOURCEDIR/TerminalMods/programs"
##custom script dir end

##phil's terminal mod start

if [[ -f "$RESOURCEDIR/TerminalMods/philTerminalMod" ]];then 
	source "$RESOURCEDIR/TerminalMods/philTerminalMod"
fi
##phil's terminal mod end

